    "@dcl/sdk": "https://sdk-team-cdn.decentraland.org/@dcl/js-sdk-toolchain/branch/feat/add-pointer-down/dcl-sdk-7.0.0-2595473629.commit-899b70f.tgz",
    "decentraland": "https://sdk-team-cdn.decentraland.org/decentraland-cli/branch/temp/sdk7/decentraland-3.10.5-20220621153604.commit-b31c6d9.tgz"
  },
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.0.0",
    "yarn": "please use npm"
  },
  "dependencies": {
    "@dcl/posix": "^1.0.4"
